                  <div class="input-group-btn">
                    <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                  </div>
                </div>
              </div>
            </div>
            <div class="container-fluid">
                <form action="" method="POST">
                    <div class="form-row">
                        <div class="col-md-4">
                        <label for="validationCustom01">Full Name</label>
                        <input type="text" class="ntrol form-control" name="fullname" value="<%= allApp.fullname%>" id="validationCustom01"  readonly>
                        </div>
                    
                    </div>
                    <div class="form-row">
                        <div class="col-md-4">
                            <label for="validationCustom03">Date of Birth</label>
                            <input type="date" class="form-control" name="dateofBirth" value="<%= allApp.dateofBirth%>" id="validationCustom03"  readonly>
                        </div>
                        <div class="col-md-4">
                            <label for="validationCustomUsername">Email</label>
