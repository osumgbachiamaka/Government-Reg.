                                <div class="col-md-4">
                                    <label for="validationCustom04">Course of Study</label>
                                    <input type="text" class="form-control" name="CourseStudy" value="<%= allApp.CourseStudy%>" id="validationCustom04" readonly>
                                </div>
                            </div>
