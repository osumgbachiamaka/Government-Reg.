                                <label for="validationCustom03">Area of interest?</label>
                                <input type="date" class="form-control" name="areaofInterest" value="<%= allApp.areaofInterest%>" id="validationCustom03" readonly>
                            </div>
                            <div class="col-md-4">
                                <label for="validationCustomUsername">School Attended</label>
                                    <input type="email" class="form-control" name="schlAttended" value="<%= allApp.schlAttended%>" id="validationCustomUsername" id="validationCustomUsername" readonly>
