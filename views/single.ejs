                                    <input type="date" class="form-control" name="School" value="<%= allApp.School%>" id="validationCustom03" readonly>
                                </div>
                                <div class="col-md-4">
                                    <label for="validationCustomUsername">Higher Education</label>
                                        <input type="email" class="form-control" name="HigherE" value="<%= allApp.HigherE%>" id="validationCustomUsername" id="validationCustomUsername" readonly>
