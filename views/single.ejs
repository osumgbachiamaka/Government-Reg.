                                <div class="col-md-4 mb-3">
                                    <label for="validationCustom03">Grade/CGPA</label>
                                    <input type="number" class="form-control" name="Grade" value="<%= allApp.Grade%>" id="validationCustom03" readonly>
                                </div>
                                <div class="col-md-4 mb-3">
                                    <label for="validationCustom03">Year admitted</label>
                                    <input type="text" class="form-control" name="yearAdmitted" value="<%= allApp.yearAdmitted%>" id="validationCustom03" readonly>
                                </div>
                                <div class="col-md-4 mb-3">
