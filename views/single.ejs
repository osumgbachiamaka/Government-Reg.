                        <div class="col-md-4">
                            <label for="validationCustom04">Age</label>
                            <input type="text" class="form-control" name="age" value="<%= allApp.age%>" id="validationCustom04" readonly>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="col-md-4 mb-3">
                            <label for="validationCustom03">Phone</label>
                            <input type="number" class="form-control" name="phone" value="<%= allApp.phone%>" id="validationCustom03" readonly>
                        </div>
                        <div class="col-md-4 mb-3">
                            <label for="validationCustom03">Village</label>
                            <input type="text" class="form-control" name="village" value="<%= allApp.village%>" id="validationCustom03" readonly>
